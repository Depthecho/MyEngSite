#: .\grammarpage\templates\grammarpage\verbs\active_voice.html:229
msgid "Present Continuous Active:"
msgstr "Настоящее длительное время (активный залог):"

#: .\grammarpage\templates\grammarpage\verbs\active_voice.html:229
msgid "Subject + am/is/are + -ing Verb"
msgstr "Подлежащее + am/is/are + Глагол с окончанием -ing"

#: .\grammarpage\templates\grammarpage\verbs\active_voice.html:235
msgid "Past Continuous Active:"
msgstr "Прошедшее длительное время (активный залог):"

#: .\grammarpage\templates\grammarpage\verbs\active_voice.html:235
msgid "Subject + was/were + -ing Verb"
msgstr "Подлежащее + was/were + Глагол с окончанием -ing"

#: .\grammarpage\templates\grammarpage\verbs\active_voice.html:241
msgid "Future Continuous Active:"
msgstr "Будущее длительное время (активный залог):"

#: .\grammarpage\templates\grammarpage\verbs\active_voice.html:241
msgid "Subject + will be + -ing Verb"
msgstr "Подлежащее + will be + Глагол с окончанием -ing"

#: .\grammarpage\templates\grammarpage\verbs\active_voice.html:249
#: .\grammarpage\templates\grammarpage\verbs\passive_voice.html:252
msgid "3. Perfect Tenses"
msgstr "3. Совершенные времена"

#: .\grammarpage\templates\grammarpage\verbs\active_voice.html:251
msgid "Present Perfect Active:"
msgstr "Настоящее совершенное время (активный залог):"

#: .\grammarpage\templates\grammarpage\verbs\active_voice.html:251
msgid "Subject + has/have + Past Participle"
msgstr "Подлежащее + has/have + Причастие прошедшего времени"

#: .\grammarpage\templates\grammarpage\verbs\active_voice.html:257
msgid "Past Perfect Active:"
msgstr "Прошедшее совершенное время (активный залог):"

#: .\grammarpage\templates\grammarpage\verbs\active_voice.html:257
msgid "Subject + had + Past Participle"
msgstr "Подлежащее + had + Причастие прошедшего времени"

#: .\grammarpage\templates\grammarpage\verbs\active_voice.html:263
msgid "Future Perfect Active:"
msgstr "Будущее совершенное время (активный залог):"

#: .\grammarpage\templates\grammarpage\verbs\active_voice.html:263
msgid "Subject + will have + Past Participle"
msgstr "Подлежащее + will have + Причастие прошедшего времени"

#: .\grammarpage\templates\grammarpage\verbs\active_voice.html:271
msgid "4. Perfect Continuous Tenses"
msgstr "4. Совершенные длительные времена"

