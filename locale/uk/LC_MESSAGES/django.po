msgstr "Тривалий Умовний Час (Conditional Continuous)"

#: .\grammarpage\templates\grammarpage\verbs\the_sequence_of_tenses_general_rule.html:260
msgid "Conditional Perfect"
msgstr "Доконаний Умовний Час (Conditional Perfect)"

#: .\grammarpage\templates\grammarpage\verbs\the_sequence_of_tenses_general_rule.html:261
msgid "Future Perfect Continuous"
msgstr "Майбутній Доконано-Тривалий Час (Future Perfect Continuous)"

#: .\grammarpage\templates\grammarpage\verbs\the_sequence_of_tenses_general_rule.html:261
msgid "Conditional Perfect Continuous"
msgstr "Доконано-Тривалий Умовний Час (Conditional Perfect Continuous)"

#: .\grammarpage\templates\grammarpage\verbs\the_sequence_of_tenses_general_rule.html:268
msgid "Exceptions to the Rule"
msgstr "Винятки з Правила"

#: .\grammarpage\templates\grammarpage\verbs\the_sequence_of_tenses_general_rule.html:270
